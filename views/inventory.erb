<h1>Inventory Page</h1>

<table>
  <tr>
    <th>Album</th>
    <th></th>
    <th>Artist</th>
    <th></th>
    <th>Quantity</th>
    <th></th>
    <th>Stock Level</th>
  </tr>
  <% for album in @albums %>
  <tr>
    <td><%= album.title %></td>
    <td> | </td>
    <td><%= album.artist_name %></td>
    <td> | </td>
    <td><%= album.quantity %></td>
    <td> | </td>
    <td class = "<%=album.stock_level%>"><%= album.stock_level %></td>

    <% end %>
  </tr>

</table>